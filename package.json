{
  "name": "moodmapper",
  "private": true,
  "type": "module",
  "scripts": {
    "build:staging": "NUXT_APP_BASE_URL=/moodmapper/  NUXT_ENV=staging nuxt build",
    "build:production": "NUXT_APP_BASE_URL=/moodmapper/  NUXT_ENV=production nuxt build",
    "dev": "NUXT_ENV=development  nuxt dev  --dotenv .env.development",
    "dev-certs": "sudo ./certs.sh",
    "generate": "NUXT_APP_BASE_URL=/moodmapper/ nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "nuxt": "^3.13.2",
    "vue": "latest"
  },
  "version": "2024-08"
}